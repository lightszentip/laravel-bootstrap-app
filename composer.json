{
    "name": "laravel/laravel",
    "type": "project",
    "description": "A example laravel application with bootstrap",
    "keywords": ["framework", "laravel", "bootstrap"],
    "license": "MIT",
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "fruitcake/laravel-cors": "^v3.0.0",
        "graham-campbell/security": "10.0",
        "guzzlehttp/guzzle": "^7.5.0",
        "laravel/framework": "^9.39.0",
        "laravel/jetstream": "^v2.12.6",
        "laravel/sanctum": "^v3.0.1",
        "laravel/tinker": "^v2.7.3",
        "laravelcollective/html": "^6.3",
        "lightszentip/laravel-release-changelog-generator": "^0.2.1",
        "livewire/livewire": "^v2.10.7",
        "spatie/laravel-permission": "^5.7"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "fakerphp/faker": "^v1.20.0",
        "friendsofphp/php-cs-fixer": "^3.13",
        "laravel/sail": "^v1.16.3",
        "mockery/mockery": "^1.5.1",
        "mrdebug/crudgen": "^1.9.2",
        "nunomaduro/collision": "^v6.3.1",
        "nunomaduro/larastan": "2.3.4",
        "pestphp/pest-plugin-laravel": "^1.3",
        "phpat/phpat": "^0.10.2",
        "phpcheckstyle/phpcheckstyle": "^0.14.8",
        "phploc/phploc": "^7.0",
        "phpmd/phpmd": "^2.13",
        "phpunit/phpunit": "^9.5.27",
        "psalm/plugin-laravel": "^2.0",
        "psalm/plugin-phpunit": "^0.18.4",
        "romanzipp/laravel-model-doc": "^1.2",
        "vimeo/psalm": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
